  "scripts": {
    "debug": "scripts/debug.sh",
    "build": "npm run clean:build && cross-env NODE_ENV=production webpack --config ./config/webpack.prod.config.js --color -p --hide-modules --display-optimization-bailout",
    "build:dev": "cross-env NODE_ENV=development webpack --config ./config/webpack.dev.config.js --color -p --hide-modules --display-optimization-bailout",
    "start": "scripts/start.sh",
    "start:dev": "cross-env NODE_ENV=development webpack-dev-server --config ./config/webpack.dev.config.js --hot --inline --content-base=dist --history-api-fallback  --port 3000",
    "dev": "concurrently \"npm run start\" \"npm run start:dev\"",
    "prod": "npm-run-all build start",
    "clean": "scripts/clean.sh",
    "clean:build": "rimraf ./dist",
    "clean:test": "rimraf ./coverage",
    "clean:all": "npm-run-all clean:build clean:test clean",
    "lint": "npm run eslint",
    "eslint": "node scripts/lint-spec.js; eslint src --ext .js,.jsx",
    "eslint:client:fix": "eslint --fix 'src/client/**/*.js' 'src/client/**/*.jsx' 'src/client/*.js'",
    "eslint:server:fix": "eslint --fix 'src/server/**/*.js'",
    "eslint:fix": "npm-run-all eslint:client:fix eslint:server:fix",
    "stylelint": "stylelint --syntax scss ./src/client/**/*.scss",
    "stylelint:fix": "stylelint --syntax scss ./src/client/**/*.scss --fix",
    "test": "mkdir -p src/server/logs/ && touch src/server/logs/conversation_discovery.log && touch src/server/logs/conversation-discovery.request.log && cross-env NODE_ENV=test node_modules/.bin/jest --config jest.config.js --verbose",
    "coverage": "npm run test:coverage",
    "test:coverage": "npm run test -- --coverage",
    "test:watch": "npm run test -- --watchAll",
    "test:update": "npm run test -- --updateSnapshot",
    "test:ui": "scripts/test-ui.sh",
    "quality": "npm-run-all clean:test eslint stylelint test:coverage",
    "prof": "scripts/prof.sh",
    "zip": "./zip.sh"
  },
